{
  "name": "Empanadas.io",
  "version": "1.3.0",
  "description": "The only app for all your Empanada needs!",
  "main": "main.js",
  "publisherName": "Empanadas.io",
  "scripts": {
    "start": "electron --enable-accelerated-mjpeg-decode --enable-accelerated-video --ignore-gpu-blacklist --enable-native-gpu-memory-buffers --enable-gpu-rasterization .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Zachary Duda (https://zachduda.github.io)",
  "license": "ISC",
  "devDependencies": {
    "electron": "^13.6.3",
    "electron-builder": "^22.13.1",
    "electron-packager": "^15.4.0"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "https://empanadas.io/download"
      }
    ],
    "appId": "empanadas.io",
    "mac": {
      "category": "games",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "win": {
      "target": "nsis",
      "verifyUpdateCodeSignature": false
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "dependencies": {
    "v8-compile-cache": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zachduda/Empanadas.io-App.git"
  },
  "bugs": {
    "url": "https://github.com/zachduda/Empanadas.io-App/issues"
  },
  "homepage": "https://empanadas.io/download"
}
